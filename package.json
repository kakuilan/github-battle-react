{
  "name": "github-battle-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.17.10",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@jam3/gsap-promise": "^4.0.1",
    "@svgr/webpack": "6.2.1",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "antd": "^4.20.2",
    "antd-mobile": "^5.11.2",
    "axios": "^0.27.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "8.2.5",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "camelcase": "^6.3.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "compression-webpack-plugin": "^9.2.0",
    "connected-react-router": "^6.9.2",
    "css-loader": "^6.7.1",
    "dotenv": "16.0.0",
    "dotenv-expand": "8.0.3",
    "eslint-config-react-app": "^7.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.1.0",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "^8.1.0",
    "is-wsl": "^2.2.0",
    "jest": "28.1.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "28.1.0",
    "jest-watch-typeahead": "1.1.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "md5.js": "^1.3.5",
    "mini-css-extract-plugin": "2.6.0",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "pnp-webpack-plugin": "1.7.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "6.2.1",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "7.5.0",
    "postcss-safe-parser": "6.0.0",
    "prop-types": "^15.8.1",
    "qs": "^6.10.3",
    "react": "^18.1.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.1.0",
    "react-lazy-load": "^3.1.13",
    "react-lazyload": "^3.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^8.0.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "resolve": "1.22.0",
    "resolve-url-loader": "5.0.0",
    "sass-loader": "12.6.0",
    "semver": "7.3.7",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-pnp": "1.2.0",
    "url-loader": "4.1.1",
    "webpack": "5.72.0",
    "webpack-dev-server": "4.9.0",
    "webpack-manifest-plugin": "5.0.0",
    "workbox-webpack-plugin": "6.5.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:dev": "dotenv -e .env.development node scripts/build.js",
    "build:test": "dotenv -e .env.test node scripts/build.js",
    "build:prod": "dotenv -e .env.production node scripts/build.js",
    "test": "node scripts/test.js",
    "analyze-src": "source-map-explorer build/static/js/main.*",
    "analyze-all": "source-map-explorer build/static/js/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "D:\\_myServer\\node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd-mobile",
          "style": "css"
        }
      ]
    ]
  },
  "devDependencies": {
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.15.0",
    "eslint-plugin-react": "^7.29.4"
  },
  "theme": {
    "brand-primary": "red",
    "color-text-base": "#333"
  }
}
